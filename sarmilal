class player:
def play(self):
printf("The player is playing cricket.")

class Batsman(player):
def play(self):
printf("The batsman is batting.")

class Bowler(player):
def play(self):
printf("The bowler is bowling.")

#create objects of both 
Batsman and Bowler classes and call the play() methopd for each object.
batsman = Batsman()
bowler = Bowler()

batsman.play() # This will
print"The batsman is batting."
bowler.play() # This will 
print"The bowler is bowling."
