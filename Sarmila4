class Student:
def __init__(self, name, roll_number, cgpa):
self.name = name
self.roll_ number = roll_number
self.cgpa = cgpa
def
sort_students(student_list):
sorted_student= sorted(student_list, key=lambda student:
student.cgpa, reverse=True)
return sorted_students
# create a list of student objects
students = [
student("Alice", "A001", 3.8),
student("Bob", "B002", 3.6),
student("Charlie", "C003", 3.9),
student("David", "D004", 3.7),
]

# sort the student by CGPA sorted_student = sort_students(students)

#print the sorted list for student in sorted_students:
print(f"Name:
{student.name}, Roll Number:
{student.roll_number}, CGPA:
{student.cgpa}")
